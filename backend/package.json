  "scripts": {
    "start:weather-app": "nest start",
    "start:dev:weather-app": "ts-node -r tsconfig-paths/register apps/weather-app/src/main.ts",
    "start:prod:weather-app": "node dist/apps/weather-app/src/main",
    "build:weather-app": "nest build weather-app",
    "start:notifications": "nest start notifications",
    "start:dev:notifications": "ts-node -r tsconfig-paths/register apps/notifications/src/main.ts",
    "start:prod:notifications": "node dist/apps/notifications/src/main",
    "build:notifications": "nest build notifications",
    "migrate:weather-app": "cd ./apps/weather-app/ && npx mikro-orm migration:up",
    "lint": "npx eslint .",
    "lint:fix": "npx eslint . --fix",
    "format": "npx prettier . --check",
    "format:fix": "npx prettier . --write",
    "test:unit:weather-app": "jest --config ./apps/weather-app/test/jest-unit.json",
    "test:architecture:weather-app": "jest --config ./apps/weather-app/test/jest-architecture.json",
    "test:integration:weather-app": "jest --config ./apps/weather-app/test/jest-integration.json",
    "test:unit:notifications": "jest --config ./apps/notifications/test/jest-unit.json",
    "test:architecture:notifications": "jest --config ./apps/notifications/test/jest-architecture.json",
    "test:integration:notifications": "jest --config ./apps/notifications/test/jest-integration.json"
  },
